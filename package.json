{
  "name": "@waytohealth/inbox-component",
  "main": "dist/inbox-component.ssr.js",
  "browser": "dist/inbox-component.esm.js",
  "module": "dist/inbox-component.esm.js",
  "unpkg": "dist/inbox-component.min.js",
  "files": [
    "dist/*"
  ],
  "sideEffects": false,
  "scripts": {
